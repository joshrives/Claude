# Required: Anthropic Admin API key (generate at https://console.anthropic.com/settings/admin-keys)
ANTHROPIC_ADMIN_API_KEY=sk-ant-admin-your-key-here

# Optional: server port (default: 3000)
PORT=3000

# Optional: how often to re-fetch from the API in milliseconds (default: 300000 = 5 min)
POLL_INTERVAL_MS=300000

# Optional: number of days to include in "All Time" column (default: 90)
ALL_TIME_DAYS=90
